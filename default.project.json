{
	"name": "roblox-ts-game",
	"globIgnorePaths": ["**/package.json", "**/tsconfig.json"],
	"emitLegacyScripts": false,
	"tree": {
		"$className": "DataModel",
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"rbxts_include": {
				"$path": "include",
				"node_modules": {
					"$className": "Folder",
					"@rbxts": {
						"$path": "node_modules/@rbxts"
					}
				}
			},
			"Project": {
				"$path": "out/modeling"
			}
		},
		"ServerScriptService": {
			"$className": "ServerScriptService",
			"run-tests": {
				"$path": "dev"
			}
		},
		"HttpService": {
            "$className": "HttpService",
            "$properties": {
                "HttpEnabled": true
            }
        }
	}
}
